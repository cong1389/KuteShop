<!----BannerTop-->
@using App.Core.Extensions
@using App.Framework.Utilities
@model IEnumerable<App.Domain.Ads.Banner>
@if (Model.IsAny())
{
    var item = Model.FirstOrDefault();
    <section class="page-title" style="background-image: url(@(Url.Content(item.ImgPath.ValidateImage()))) !important;">
        <div class="auto-container">
            <h1>@item.Title</h1>
           
        </div>
    </section>
}
<!----/BannerTop-->