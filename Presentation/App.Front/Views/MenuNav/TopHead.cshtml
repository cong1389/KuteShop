<!---TopHead-->
@using App.Framework.Utilities
@using Microsoft.AspNet.Identity
@model IEnumerable<App.FakeEntity.Menus.MenuNavViewModel>

<!-- Header Top -->

<div class="header-top">

    <div class="auto-container clearfix">

        <!-- Top Left -->

        <div class="top-left">

            <ul class="clearfix">
                @if (Model != null)
                {
                    var i = 0;
                    foreach (var lv1 in Model)
                    {
                        <a href="@Url.Action("GetContent", "Menu", new { menu = lv1.SeoUrl, area = "" })">
                            <img alt="@lv1.MenuName" src="@lv1.ImageSmallSize.ValidateImage()" />@lv1.MenuName
                        </a>
                        i++;
                    }
                }

                <li><a href="index.html">We have more than 25 years of experiences</a></li>


            </ul>

        </div>



        <!-- Top Right -->





    </div>

</div>
<!---/TopHead-->