@using App.Framework.Utilities
@using Resources
@model App.FakeEntity.GenericControls.GenericControlValueViewModel
@{
	ViewBag.Title = FormUI.Edit;
	ViewBag.Toolbar = new[] { "Save", "Divider", "Back" };
}
@using (Html.BeginForm("Edit", "GenericControlValue", FormMethod.Post, new { data_container = ViewContext.RouteData.Values["controller"], enctype = "multipart/form-data" }))
{
	@Html.AntiForgeryToken()
	@Html.HiddenFor(x => x.Id)

	Html.RenderPartial("_CreateOrUpdate", Model);
}
@section scripts{
	<script src="@Url.FakePathAdmin("scripts/plugins/jquery-minicolors/jquery.minicolors.min.js")"></script>
	<script src="@Url.FakePathAdmin("scripts/components-color-pickers.js")"></script>
}
@section style{
	<link href="@Url.FakePathAdmin("content/plugins/jquery-minicolors/jquery.minicolors.css")" rel="stylesheet" />
}

